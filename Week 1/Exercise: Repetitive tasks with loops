// Task 1
for(i = 1; i < 6; i++){
    console.log(i)
}
console.log("Counting completed!")

// Task 2
for(i = 5; i > 0; i--){
    console.log(i)
}
console.log("Counting finished!")

// Task 3
var i = 1
while (i < 6) {
    console.log(i)
    i++
}
console.log("Counting completed!")

// Task 4
var i = 5
while (i > 0) {
    console.log(i)
    i--
}
console.log("Counting finished!")

// Task 5
var year = 2018
while (year <2023) {
    console.log(year)
    year++
}